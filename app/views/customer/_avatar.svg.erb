
<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" fill="none" viewBox="0 0 24 24">
  <defs>
    <circle id="circ" cx="50%" cy="50%" r="50%" />
    <clipPath id="clip">
      <use xlink:href="#circ"/>
    </clipPath>
  </defs>
  <image xlink:href="<%= url %>" width="100%" height="100%" clip-path="url(#clip)"/>
</svg>